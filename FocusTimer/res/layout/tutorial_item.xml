<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:geekui="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_tutorial_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/img_tutorial_01_3x"
    android:orientation="vertical" >

    <com.mobsandgeeks.ui.TypefaceTextView
        android:id="@+id/tv_tutorial"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_gravity="center"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="64dp"
        android:gravity="center"
        android:text="Text"
        android:textColor="@color/ft_white"
        android:textSize="27dp"
        geekui:customTypeface="fonts/NanumBarunGothicLight.ttf" />

    <ImageButton
        android:id="@+id/btn_tutorial_start"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="64dp"
        android:background="@color/transparent"
        android:src="@drawable/ic_start_btn_3x" />

</RelativeLayout>